{
    "manifest_version":3,
    "name":"AKatze",
    "version":"1.0",
    "description":"砂猫浏览器宠物",
    "action":{
        "default_icon":"data/icon.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "css":["style.css"],
            "js":["content.js"]
        }
    ],
    "web_accessible_resources":[{
        "resources":[
            "data/*"
        ],
        "matches":["<all_urls>"]
    }]
}